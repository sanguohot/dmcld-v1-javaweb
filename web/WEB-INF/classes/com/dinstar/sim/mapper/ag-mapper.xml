    <select id="getAgAllInfo" resultMap="agMapperMap">
		select tn.*,
		ta.uuid as ag_uuid
		from tbl_ne tn, tbl_ag ta 
		where tn.uuid=ta.ne_uuid
		AND (tn.rec_status IS NULL OR tn.rec_status!=2)
		AND (tn.product_id=17 || tn.product_id=18 || tn.product_id=19
		 || tn.product_id=11 || tn.product_id=12 || tn.product_id=15 || tn.product_id=16)
		<if test="uuid > 0" >
        AND  tn.uuid = #{uuid,jdbcType=INTEGER}
        </if>
        <if test="domainUuid > 0" >
        AND  tn.domain_uuid = #{domainUuid}
        </if>
        <if test="productSn != null" >
        AND  tn.product_sn = #{productSn}
        </if>
        <if test="alias!=null and alias!=''" >
        AND  tn.alias like #{alias}
        </if>
        
        <if test="limit > 0" >
        LIMIT #{start},#{limit}
        </if>
    </select>
    
